{% extends "./front-end-base.njk" %}

{% block body %}
    {% for post in posts %}
        <article>
            <h1 style="font-size:2em"><a href="/p/{{ post.id }}">{{ post.title }}</a></h1>
            {{ post.preview | safe}}
        </article>
    {% else %}
        <p>No Post</p>
    {% endfor %}
{% endblock %}